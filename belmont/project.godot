; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="adastal-belmont"
run/main_scene="res://boot.tscn"
config/features=PackedStringArray("4.2", "Mobile")
run/low_processor_mode=true
boot_splash/bg_color=Color(0, 0.392157, 0.639216, 1)
config/icon="res://icon.svg"

[display]

window/size/viewport_width=770
window/size/viewport_height=350
window/stretch/mode="2d"
window/stretch/aspect="expand"
window/per_pixel_transparency/allowed=true

[editor]

movie_writer/movie_file="/tmp/ad.avi"
movie_writer/fps=95

[rendering]

renderer/rendering_method="gl_compatibility"
environment/defaults/default_clear_color=Color(0, 0, 0, 1)
threads/thread_model=2
quality/filters/use_nearest_mipmap_filter=true
quality/filters/msaa=4
quality/filters/use_fxaa=true
quality/filters/use_debanding=true
